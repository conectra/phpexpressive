{
  "database": {
    "repository": "pessoa",
    "keys": "ID"
  },
  "properties": [
    {
      "alias": "proID",
      "property": "ID",
      "type": "int",
      "behavior": {
        "required": "false",
        "autoIncrement": "true",
        "whenReplicate": {
          "action": "last+1"
        }
      }
    },
    {
      "alias": "proNome",
      "property": "nome",
      "type": "string"
    },
    {
      "alias": "proCodigo",
      "property": "codigo",
      "type": "int",
      "behavior": {
        "whenReplicate" : {
          "action" : "last+1"
        }
      }
    },
    {
      "alias": "proInscricaoFederal",
      "property": "inscricaoFederal",
      "type": "string",
      "behavior": {
        "required": "false",
        "whenReplicate": {
          "action": "clean"
        }
      }
    },
    {
      "alias": "proCidade",
      "property": "cidade",
      "type": "json",
      "composition": {
        "class": "Solis\\Expressive\\Sample\\Cidade\\Repository\\Cidade"
      }
    }
  ]
}